
logo_predator() {
    mkdir -p "$CACHE/logo" > /dev/null 2>&1
    rm -f    "$CACHE/logo/predator.svg" > /dev/null 2>&1
	cp -f "$TEMPLATES/logo/predator.svg" "$CACHE/logo/predator.svg" || return 1
	sed -i "s/#000000/${COLOR[2]}/gi" "$CACHE/logo/predator.svg" || return 1
}


normal "[ ] Install predator logo"
logo_predator && \
    success "[+] Predator logo installed" || \
    error   "[-] Predator logo not installed"

        ButtonFace="Button"
        ButtonFace2="Button" // for use in main client list panel column header, some button states
        ButtonFace3="Button" // button cornering pixels
        ButtonFaceDisabled="ButtonInactive"
        ButtonFaceHover="ButtonHover"   /// hover!
        ButtonFaceActive="ButtonHover"  // not sure what this state is...
        ButtonFaceFocus="Button"  //  keyboard focus

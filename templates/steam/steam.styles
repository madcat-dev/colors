    styles {
        downarrow {
            image="graphics/icon_down_default"
        }

        downarrow:hover {
            image="graphics/icon_down_hover"
            render_bg {
                //background
                0="fill(x0, y0, x1, y1, ButtonHover)"
            }
        }

        uparrow {
            image="graphics/icon_up_default"
        }

        rightarrow {
            image="graphics/icon_right_default"
        }

        rightarrow:hover {
            image="graphics/icon_right_hover"
        }

        Button {
            font-family=basefont
            font-size=13
            font-weight=400
            textcolor="Text"
            font-style=uppercase
            bgcolor=none
            render_bg {
                0="fill(x0, y0, x1, y1, Button)"
            }
        }

        "Page Button" {
            render_bg {
                0="fill(x0, y0, x1, y1, Button)"
            }
        }

        Button:hover {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, ButtonHover)"
            }
        }

        "Page Button:hover" {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, ButtonHover)"
            }
        }

        Button:focus {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, Button)"
            }
        }

        "Page Button:focus" {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1,  Button)"
            }
        }

        Button:active { // this is the left-mouse-currently-pressed state
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, Background)"
            }
        }

        "Page Button:active" {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, Background)"
            }
        }

        Button:selected {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, Button)"
            }
        }

        "Page Button:selected" {
            textcolor="TextHover"
            render_bg {
                0="fill(x0, y0, x1, y1, Button)"
            }
        }

        Button:disabled {
            textcolor="Text"
            render_bg {
                // background fill
                0="fill( x0, y0, x1, y1, ButtonInactive)"
            }
        }

        "Page Button:disabled" {
            textcolor="Text"
            render_bg {
                // background fill
                0="fill( x0, y0, x1, y1, ButtonInactive)"

steam.styles {
    colors {
        // colors section can include colors in "R G B A" form, or references to already defined colors;
        // it can also include just general settings controls can pull from for extra customization
        none="0 0 0 0"

		black="$(rgb `get 0`) 255"
		red="$(rgb `get 1`) 255"
		green="$(rgb `get 2`) 255"
		yellow="$(rgb `get 3`) 255"
		blue="$(rgb `get 4`) 255"
		magenta="$(rgb `get 5`) 255"
		purple="$(rgb `get 5`) 255"
		cyan="$(rgb `get 6`) 255"
		white="$(rgb `get 7`) 255"


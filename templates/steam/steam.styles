        // cool we can define font-families here as well
        basefont="Arial"
        basefont="Helvetica" [\$OSX]
        basefont="Noto Sans" [\$LINUX]



		Background="$(rgb `get background`) 255"
		Foreground="$(rgb `get foreground`) 255"
		Selected="$(rgb `get cursor`) 255"

		Button="$(rgb $(rgb_value `get background` +3)) 255"
		ButtonHover="Selected"
		ButtonInactive="$(rgb $(rgb_value `get background` +20)) 255"

		Text="$(rgb `get foreground`) 255"
		TextHover="$(rgb `get 15`) 255"
		TextInactive="$(rgb $(rgb_value `get background` +20)) 255"



